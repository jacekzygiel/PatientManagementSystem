{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "@model": ["src/model/index"],
      "@functions": ["src/functions/index"],
      "@testData": ["tests/testData/index"]
    }
  }
}
